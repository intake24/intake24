    "nodemon-webpack-plugin": "^4.8.2",
    "npm-run-all2": "^8.0.4",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^6.1.0",
    "sass": "^1.94.0",
    "supertest": "^7.1.4",
    "ts-loader": "^9.5.4",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^3.2.4",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^7.0.0"
