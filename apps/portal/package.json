    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "express": "^5.1.0",
    "fs-extra": "^11.3.2",
    "helmet": "^8.1.0",
    "jsdom": "^27.2.0",
    "morgan": "^1.10.1",
    "nunjucks": "^3.2.4",
    "sitemap": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.10",
    "@types/nunjucks": "^3.2.6",
