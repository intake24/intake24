{
  "name": "@intake24/food-tools",
  "type": "module",
  "version": "2025.2",
  "description": "Food import and validation tools for Intake24",
  "author": {
    "name": "Action Lab"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
