    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^20.14.14",
    "rimraf": "^6.0.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
